  "testScenarios": {
    "PRE_01_Create": {
      "description": "Create a preset with all parameters",
      "steps": [
        "Fill model name: gpt-4",
        "Add prompt text",
        "Set temperature: 0.7",
        "Set max tokens: 1000",
        "Toggle web search: on",
        "Save preset",
        "Verify appears in list with correct values"
      ],
      "expected": "Preset saved and usable; values persisted"
    },
    "PRE_02_Activate": {
      "description": "Activate preset and verify settings applied",
      "controlQuestion": "Explain quantum computing",
      "steps": [
        "Ask control question without preset (capture baseline)",
        "Select 'Quick Response' preset (temp=0.3, max=500)",
        "Ask same question",
        "Compare: should be shorter and more direct"
      ],
      "expected": "Model and settings applied; observable differences"
    },
    "PRE_03_Update": {
      "description": "Update preset in active session",
      "steps": [
        "Open session with 'Creative Writer' preset (temp=0.9)",
        "Ask: 'Write a story about AI'",
        "Update preset: change temp to 0.3",
        "Apply to active session",
        "Ask same question",
        "Compare: second response less creative, more factual"
      ],
      "expected": "New settings influence responses immediately"
    },
    "PRE_04_Search_Dedup": {
      "description": "Search presets and prevent duplicates",
      "tests": [
        {
          "searchTerm": "GPT-4",
          "expectedResults": ["preset_001", "preset_004"]
        },
        {
          "searchTerm": "creative",
          "expectedResults": ["preset_002"]
        },
        {
          "searchTerm": "עברית",
          "expectedResults": ["preset_004"]
        },
        {
          "duplicateTest": "Attempt to create preset with name 'Quick Response (GPT-4)'",
          "expected": "Duplicate prevented or warning shown"
        }
      ]
    }
  }
}
